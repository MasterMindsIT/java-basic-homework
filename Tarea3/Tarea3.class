import java.util.Scanner;

public class Tarea3 {


    public static void main(String[] args) {
        String startText = "";
        Scanner scanner = new Scanner(System.in);
        System.out.print("Introducir un texto por favor: ");
        String scannerText = scanner.nextLine();
        startText += scannerText + " ";
        String[] arrayText = new String[]{"Texto1", "Texto2", "Texto3", "Texto4 "};
        for (int i = 0; i <= arrayText.length -1; i++) {
            startText += arrayText[i] + " ";
        }
        System.out.println("El texto concatenado es : " + startText);

    }
}

